{
    "name": "babdev/laravel-breadcrumbs",
    "license": "MIT",
    "description": "Adds breadcrumb support to your Laravel applications.",
    "keywords": [
        "laravel",
        "breadcrumb"
    ],
    "require": {
        "php": "^8.0",
        "facade/ignition-contracts": "^1.0",
        "illuminate/events": "^9.0",
        "illuminate/filesystem": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/view": "^9.0"
    },
    "require-dev": {
        "laravel/framework": "^9.0",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\Breadcrumbs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\Breadcrumbs\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BabDev\\Breadcrumbs\\Providers\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "BabDev\\Breadcrumbs\\Facades\\Breadcrumbs"
            }
        }
    }
}
