{
    "name": "babdev/laravel-breadcrumbs",
    "license": "MIT",
    "description": "Adds breadcrumb support to your Laravel applications.",
    "keywords": [
        "laravel",
        "breadcrumb"
    ],
    "require": {
        "php": "^7.2",
        "facade/ignition-contracts": "^1.0",
        "illuminate/routing": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "illuminate/view": "^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16.3",
        "laravel/framework": "^6.0|^7.0",
        "orchestra/testbench": ">=3.6",
        "phpunit/phpunit": "^7.0|^8.0",
        "php-coveralls/php-coveralls": "^2.0",
        "spatie/phpunit-snapshot-assertions": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\Breadcrumbs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/",
            "BabDev\\Breadcrumbs\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BabDev\\Breadcrumbs\\Providers\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "BabDev\\Breadcrumbs\\Facades\\Breadcrumbs"
            }
        }
    }
}
