{
    "name": "babdev/laravel-breadcrumbs",
    "license": "MIT",
    "description": "Adds breadcrumb support to your Laravel applications.",
    "keywords": [
        "laravel",
        "breadcrumb"
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "facade/ignition-contracts": "^1.0",
        "illuminate/events": "^6.18 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/filesystem": "^6.18 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/routing": "^6.18 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "^6.18 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/view": "^6.18 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "laravel/framework": "^6.18 || ^8.0 || ^9.0 || ^10.0",
        "orchestra/testbench": "^4.0 || ^6.0 || ^7.0 || ^8.0",
        "phpunit/phpunit": "^8.5 || ^9.6 || ^10.0",
        "spatie/phpunit-snapshot-assertions": "^3.0 || ^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "BabDev\\Breadcrumbs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BabDev\\Breadcrumbs\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "BabDev\\Breadcrumbs\\Providers\\BreadcrumbsServiceProvider"
            ],
            "aliases": {
                "Breadcrumbs": "BabDev\\Breadcrumbs\\Facades\\Breadcrumbs"
            }
        }
    }
}
